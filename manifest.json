{
  "manifest_version": 3,
  "name": "ENS App Favorites Exporting Tool",
  "version": "1.8",
  "description": "An extension to export your favorites list from the official ENS Domains app to a convenient .txt file.",
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  },
  "permissions": [
    "activeTab",
    "scripting"
  ],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["confetti.js"]
    }
  ]
}